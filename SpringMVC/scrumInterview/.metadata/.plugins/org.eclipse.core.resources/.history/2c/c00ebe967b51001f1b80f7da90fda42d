package com.example.demo;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.context.ConfigurableApplicationContext;

import com.example.demo.repository.StudentRepo;

@SpringBootApplication
public class MarshellingUnMarshellingProjectApplication {

	public static void main(String[] args) {
		ConfigurableApplicationContext run = SpringApplication.run(MarshellingUnMarshellingProjectApplication.class, args);
		
		StudentRepo bean = run.getBean(StudentRepo.class);
	}

}
